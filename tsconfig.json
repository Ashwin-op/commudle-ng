{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "lib-authwatch": [
        "dist/lib-authwatch"
      ],
      "lib-authwatch/*": [
        "dist/lib-authwatch/*"
      ],
      "lib-server-communications": [
        "dist/lib-server-communications"
      ],
      "lib-server-communications/*": [
        "dist/lib-server-communications/*"
      ],
      "api-routes": [
        "dist/api-routes"
      ],
      "api-routes/*": [
        "dist/api-routes/*"
      ],
      "lib-error-handler": [
        "dist/lib-error-handler"
      ],
      "lib-error-handler/*": [
        "dist/lib-error-handler/*"
      ],
      "lib-api-parser": [
        "dist/lib-api-parser"
      ],
      "lib-api-parser/*": [
        "dist/lib-api-parser/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}